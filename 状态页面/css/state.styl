html,body,div,p,img,button
	padding 0
	margin 0
	text-align center
body
	background #f5f5f5
	.m-img-wrap
		margin-top 120px
		font-size 0
		.m-img
			height 141px
			width auto
	.g-txt
		margin-top 30px
		font-size 14px
		color #666
	.g-bts
		position: absolute
		bottom 140px
		left 0 
		right 0
		button
			border none 
			background none
			border-radius 10px
			padding 10px 28px
			font-size 14px
			font-weight 500
			&.m-btn-back
				border 1px solid #f23030
				margin-right 10px
				color #f23030
			&.m-btn-re
				background #f23030		
				margin-left 10px
				color #fff
				